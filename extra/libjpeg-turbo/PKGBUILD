plugrel=1

pkgname=libjpeg-turbo
pkgver=1.1.1
pkgrel=3
pkgdesc="ARM dummy package for the x86 assembly version"
arch=('arm' 'armv7h')
url="http://www.archlinuxarm.org"
depends=('libjpeg')
license=('BYOB')

build() {
  echo "libjpeg-turbo!"
}

package() {
  mkdir -p $pkgdir/usr/share
  echo "libjpeg-turbo!" > $pkgdir/usr/share/libjpeg-turbo
}
